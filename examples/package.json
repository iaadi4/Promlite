{
  "name": "prometheus-client-examples",
  "version": "1.0.0",
  "description": "Examples demonstrating prometheus-client usage with Express.js",
  "scripts": {
    "counter": "node counter/server.js",
    "gauge": "node gauge/server.js",
    "histogram": "node histogram/server.js",
    "comprehensive": "node comprehensive/server.js",
